<h1>Listing corpus_positives</h1>

<table>
  <tr>
    <th>Sentence</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @corpus_positives.each do |corpus_positive| %>
  <tr>
    <td><%= corpus_positive.sentence %></td>
    <td><%= link_to 'Show', corpus_positive %></td>
    <td><%= link_to 'Edit', edit_corpus_positive_path(corpus_positive) %></td>
    <td><%= link_to 'Destroy', corpus_positive, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

