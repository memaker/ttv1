<h1>Listing corpus_negatives</h1>

<table>
  <tr>
    <th>Sentence</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @corpus_negatives.each do |corpus_negative| %>
  <tr>
    <td><%= corpus_negative.sentence %></td>
    <td><%= link_to 'Show', corpus_negative %></td>
    <td><%= link_to 'Edit', edit_corpus_negative_path(corpus_negative) %></td>
    <td><%= link_to 'Destroy', corpus_negative, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

